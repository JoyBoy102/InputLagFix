		</TwelveSetting>
		<ThirteenSetting>
			<valuePath>HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\wercplsupport</valuePath>
			<valueName>Start</valueName>
			<value_if_true>4</value_if_true>
