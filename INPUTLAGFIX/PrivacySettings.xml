	<CortanaVoiceActivationOff ruName="Отключить активацию голосом для приложений и Cortana">
		<FirstSetting>
			<valuePath>HKCU\Software\Microsoft\Speech_OneCore\Settings\VoiceActivation\UserPreferenceForAllApps</valuePath>
			<valueName>AgentActivationEnabled</valueName>
			<value_if_true>0</value_if_true>
			<value_if_false>delete</value_if_false>
			<valueKind>REG_DWORD</valueKind>
		</FirstSetting>
		<SecondSetting>
			<valuePath>HKLM\SOFTWARE\Policies\Microsoft\Windows\AppPrivacy</valuePath>
			<valueName>LetAppsActivateWithVoice</valueName>
			<value_if_true>2</value_if_true>
			<value_if_false>delete</value_if_false>
			<valueKind>REG_DWORD</valueKind>
		</SecondSetting>
		<ThirdSetting isTask="true">
			<valuePath>Microsoft\Windows\Speech\SpeechModelDownloadTask</valuePath>
			<value_if_true>Disable</value_if_true>
			<value_if_false>Enable</value_if_false>
		</ThirdSetting>
	</CortanaVoiceActivationOff>
	<CortanaVoiceActivationOnDisabledOSOff ruName="Отключить активацию голосом для приложений и Cortana при заблокированной системе">
		<FirstSetting>
			<valuePath>HKCU\Software\Microsoft\Speech_OneCore\Settings\VoiceActivation\UserPreferenceForAllApps</valuePath>
			<valueName>AgentActivationOnLockScreenEnabled</valueName>
			<value_if_true>0</value_if_true>
			<value_if_false>delete</value_if_false>
			<valueKind>REG_DWORD</valueKind>
		</FirstSetting>
		<SecondSetting>
			<valuePath>HKLM\SOFTWARE\Policies\Microsoft\Windows\AppPrivacy</valuePath>
			<valueName>LetAppsActivateWithVoiceAboveLock</valueName>
			<value_if_true>2</value_if_true>
			<value_if_false>delete</value_if_false>
			<valueKind>REG_DWORD</valueKind>
		</SecondSetting>
	</CortanaVoiceActivationOnDisabledOSOff>
	
</PrivacySetting>
