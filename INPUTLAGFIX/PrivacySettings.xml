	<LicenseTelemetryOff ruName="Отключить телеметрию лицензии">
		<FirstSetting>
			<valuePath>HKLM\Software\Policies\Microsoft\Windows NT\CurrentVersion\Software Protection Platform</valuePath>
			<valueName>NoGenTicket</valueName>
			<value_if_true>1</value_if_true>
			<value_if_false>delete</value_if_false>
			<valueKind>REG_DWORD</valueKind>
		</FirstSetting>
	</LicenseTelemetryOff>
	<ErrorsReportWindowsOff ruName="Отключить отчет об ошибках Windows(WER)">
		<FirstSetting>
			<valuePath>HKLM\Software\Policies\Microsoft\Windows NT\CurrentVersion\Software Protection Platform</valuePath>
			<valueName>Disabled</valueName>
			<value_if_true>1</value_if_true>
			<value_if_false>delete</value_if_false>
			<valueKind>REG_DWORD</valueKind>
		</FirstSetting>
		<SecondSetting>
			<valuePath>HKLM\SOFTWARE\Microsoft\Windows\Windows Error Reporting</valuePath>
			<valueName>Disabled</valueName>
			<value_if_true>1</value_if_true>
			<value_if_false>delete</value_if_false>
			<valueKind>REG_DWORD</valueKind>
		</SecondSetting>
		<ThirdSetting>
			<valuePath>HKLM\SOFTWARE\Microsoft\Windows\Windows Error Reporting</valuePath>
			<valueName>AutoApproveOSDumps</valueName>
			<value_if_true>0</value_if_true>
			<value_if_false>delete</value_if_false>
			<valueKind>REG_DWORD</valueKind>
		</ThirdSetting>
		<FourthSetting>
			<valuePath>HKLM\SOFTWARE\Policies\Microsoft\Windows\Windows Error Reporting</valuePath>
			<valueName>AutoApproveOSDumps</valueName>
			<value_if_true>0</value_if_true>
			<value_if_false>delete</value_if_false>
			<valueKind>REG_DWORD</valueKind>
		</FourthSetting>
		<FiveSetting>
