		</FirstSetting>
	</ApplicationImpactTelemetryOff>
	<DataColletionPolicyTelemetryOff ruName="Отключить телеметрию в политике сбора данных">
		<FirstSetting>
			<valuePath>HKLM\SOFTWARE\Wow6432Node\Microsoft\Windows\CurrentVersion\Policies\DataCollection</valuePath>
			<valueName>AllowTelemetry</valueName>
			<value_if_true>0</value_if_true>
			<value_if_false>delete</value_if_false>
			<valueKind>REG_DWORD</valueKind>
		</FirstSetting>
		<SecondSetting>
			<valuePath>HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\DataCollection</valuePath>
			<valueName>AllowTelemetry</valueName>
			<value_if_true>0</value_if_true>
			<value_if_false>delete</value_if_false>
			<valueKind>REG_DWORD</valueKind>
		</SecondSetting>
		<ThirdSetting>
			<valuePath>HKLM\SOFTWARE\Policies\Microsoft\Windows\DataCollection</valuePath>
			<valueName>AllowTelemetry</valueName>
			<value_if_true>0</value_if_true>
			<value_if_false>delete</value_if_false>
